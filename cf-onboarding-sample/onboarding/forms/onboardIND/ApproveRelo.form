{
    "id": "approverelo",
    "schemaVersion": "1.1.0",
    "name": "ApproveRelo",
    "revision": "1",
    "type": "task",
    "readOnly": false,
    "content": [
        {
            "id": "personal_information",
            "type": "section",
            "title": "Personal Information",
            "content": [
                {
                    "id": "first_name",
                    "type": "string",
                    "label": "First Name",
                    "readOnly": false,
                    "value": "${context.CInfo.d.firstName}",
                    "constraints": {
                        "required": false
                    }
                },
                {
                    "id": "last_name",
                    "type": "string",
                    "label": "Last Name",
                    "readOnly": false,
                    "value": "${context.CInfo.d.lastName}",
                    "constraints": {
                        "required": false
                    }
                }
            ]
        },
        {
            "id": "unit_info",
            "type": "section",
            "title": "Unit Info",
            "content": [
                {
                    "id": "location",
                    "type": "string",
                    "label": "Location",
                    "readOnly": false,
                    "value": "${context.CInfo.d.location}",
                    "constraints": {
                        "required": false
                    }
                },
                {
                    "id": "division",
                    "type": "string",
                    "label": "Division",
                    "readOnly": false,
                    "value": "${context.CInfo.d.division}",
                    "constraints": {
                        "required": false
                    }
                },
                {
                    "id": "department",
                    "type": "string",
                    "label": "Department",
                    "readOnly": false,
                    "value": "${context.CInfo.d.department}",
                    "constraints": {
                        "required": false
                    }
                }
            ]
        },
        {
            "id": "equipment_needed",
            "type": "section",
            "title": "Equipment Needed",
            "content": [
                {
                    "id": "equipment",
                    "type": "collection",
                    "title": "Equipment",
                    "addEnabled": false,
                    "deleteEnabled": false,
                    "value": "${context.equipment.RelocationInfo}",
                    "content": [
                        {
                            "id": "product_id",
                            "type": "string",
                            "label": "Product ID",
                            "readOnly": false,
                            "value": "${item.ProductID}",
                            "constraints": {
                                "required": false
                            }
                        },
                        {
                            "id": "product_description",
                            "type": "string",
                            "label": "Product Description",
                            "readOnly": false,
                            "value": "${item.ProdDesc}",
                            "constraints": {
                                "required": false
                            }
                        },
                        {
                            "id": "type",
                            "type": "string",
                            "label": "Type",
                            "readOnly": false,
                            "value": "${item.EquipmentType}",
                            "constraints": {
                                "required": false
                            }
                        },
                        {
                            "id": "price",
                            "type": "float",
                            "label": "Price",
                            "readOnly": false,
                            "value": "${item.Price}",
                            "constraints": {
                                "required": false
                            }
                        },
                        {
                            "id": "status",
                            "type": "string",
                            "label": "Status",
                            "readOnly": false,
                            "value": "${item.Status}",
                            "constraints": {
                                "required": false
                            }
                        },
                        {
                            "id": "currency",
                            "type": "string",
                            "label": "Currency",
                            "readOnly": false,
                            "value": "${item.Currency}",
                            "constraints": {
                                "required": false
                            }
                        }
                    ]
                }
            ]
        }
    ],
    "decisions": [
        {
            "id": "approve",
            "text": "Approve",
            "decisionType": "positive"
        },
        {
            "id": "reject",
            "text": "Reject",
            "decisionType": "negative"
        }
    ]
}